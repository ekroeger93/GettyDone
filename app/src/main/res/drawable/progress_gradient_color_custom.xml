<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="#8ab8e0"
                android:centerColor="#8ab8e0"
                android:centerY="0.75"
                android:endColor="#8ab8e0"
                android:angle="270"
                />
        </shape>
    </item>


    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#b4ede0"
                    android:centerColor="#b4ede0"
                    android:centerY="0.75"
                    android:endColor="#b4ede0"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

</layer-list>

